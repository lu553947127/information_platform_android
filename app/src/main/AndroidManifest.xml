            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths_private">

            </meta-data>
        </provider>

        <!--QQ分享 start-->
        <activity
            android:name="com.tencent.tauth.AuthActivity"
